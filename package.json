    "express": "^5.0.1",
    "express-session": "^1.18.1",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "node-pg-migrate": "^7.9.1",
    "pg": "^8.14.1",
    "pg-promise": "^11.13.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ts-loader": "^9.5.2"
  }
}
